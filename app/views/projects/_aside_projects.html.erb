<% projects.each do |project| %>
  <li class="flex justify-between py-2">
    <%= link_to project.title, project, class: "grow pl-8" %>
    <%= link_to edit_project_path(project),
        data:{turbo_frame: "modal"},
        class: "px-4"  do %>
      <i class="fa-solid fa-ellipsis"></i>
    <% end %>
  </li>
<% end %>