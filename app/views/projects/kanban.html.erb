<div>
  <%= render partial: 'head', locals: {project: @project} %>
</div>
<hr>

<div class="flex gap-2">
  <ul id="all_list" class="flex gap-2" data-controller="sortable" >
    <%= render partial: 'list', collection: @project.lists.rank(:row_order)%> 
  </ul>
  <div>
    <%= link_to  new_project_list_path(@project), data: { turbo_frame: "modal" } do%>
      <i class="fa-solid fa-plus"></i>
    <% end %>
  </div>
</div>