<li class="flex">
    <div><%= link_to project.title, project_path(project.id) %></div>
    <div><%= project.description %></div>
    <div><%= User.find(project.owner_id).nick_name %></div>
    <div>
        <div><%= project.created_at.strftime("%Y-%m-%d") %></div>
        <div><%= project.updated_at.strftime("%Y-%m-%d") %></div>
    </div>
</li>